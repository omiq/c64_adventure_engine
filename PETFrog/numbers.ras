program MyProgram;
var  

	num:integer=123;
begin

	DefineScreen();	
	ClearScreen($20, ^$8000);
	
	screen_char_loc:=$8000;
	screenmemory:=$8000;
	
	moveto(0,0,hi(screen_char_loc));
	printnumber(num/100);

	moveto(0,2,hi(screen_char_loc));
	printnumber(mod(num/10,10));

	moveto(0,4,hi(screen_char_loc));
	printnumber(mod(num,10));


end.
	